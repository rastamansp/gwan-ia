# ========================================
# AMBIENTE DE PRODUÇÃO
# ========================================
NODE_ENV=production

# ========================================
# APLICAÇÃO
# ========================================
VITE_APP_NAME=Gwan IA
VITE_APP_VERSION=1.1.0

# ========================================
# API URL - PRODUÇÃO
# ========================================
VITE_API_URL=https://api.gwan.com.br/api/v1/

# ========================================
# GWAN EVENT AI AGENT - PRODUÇÃO
# ========================================
VITE_GWAN_EVENT_URL=https://events.gwan.com.br/

# ========================================
# GWAN MART AI AGENT - PRODUÇÃO
# ========================================
VITE_GWAN_MART_AI_URL=https://mcp.gwan.com.br/api/ai-agent/chat

# ========================================
# GWAN LEGAL AI AGENT - PRODUÇÃO
# ========================================
VITE_GWAN_LEGAL_AI_URL=https://legal-ai.gwan.com.br/

# ========================================
# CHAT HEALTH API - BOT JAIMINHO - PRODUÇÃO
# ========================================
VITE_CHAT_HEALTH_API_URL=http://api-events.gwan/api/chat-health

# ========================================
# BUILD (Informações de Build - Deixar vazio para auto-gerar)
# ========================================
VITE_BUILD_TIME=
VITE_GIT_COMMIT=
VITE_GIT_BRANCH=main

# ========================================
# PRODUÇÃO (Debug e Logs)
# ========================================
VITE_DEBUG=false
VITE_LOG_LEVEL=info
VITE_SHOW_DEV_TOOLS=false
VITE_LOG_FORMAT=json

# ========================================
# MONITORAMENTO APM (Variáveis VITE_ para exposição no cliente)
# ========================================
VITE_OTEL_EXPORTER_OTLP_ENDPOINT=http://gwan.com.br:4317
VITE_OTEL_SERVICE_NAME=gwan-ia-frontend
VITE_OTEL_SERVICE_VERSION=1.1.0
VITE_OTEL_RESOURCE_ATTRIBUTES=service.name=gwan-ia-frontend,service.version=1.1.0
